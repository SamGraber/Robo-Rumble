extends ../includes/layout

block main-content
	section.jumbotron
		h1 Robo Rumble

		div.row.hidden-xs
			div.col-sm-6.col-lg-4.text-center
				div.thumbnail
					img(data-src="holder.js/300x200" style="width: 300px; height: 200px; padding-right:20px" src="data:image/png;base64,")
					.caption 
						span Getting lit up by a missile
		
			div.col-sm-6.col-lg-4.text-center
				div.thumbnail
					img(data-src="holder.js/300x200" style="width: 300px; height: 200px; padding-right:20px" src="data:image/png;base64,")
					.caption
						span Grabbing the flag
		
			div.col-lg-4.visible-lg.text-center
				div.thumbnail
					img(data-src="holder.js/300x200" style="width: 300px; height: 200px; padding-right:20px" src="data:image/png;base64,")
					.caption
						span Robot on robot action

		p An online turn-based strategy game. Players build robots and program them to win contests.
						
		a.btn.btn-primary(href="/new/") Start a Game

	.row
		section.col-md-6
			h3 Capture the Flag
			p Describe capture the flag.
			a.btn.btn-default(href="/new/flag/") New Capture the Flag Game
		
		section.col-md-6
			h3 Death Matches
			p Describe death match.
			a.btn.btn-default(href="/new/death/") New Death Match Game

		section.col-md-6
			h3 King of the Hill
			p Describe king of the hill.
			a.btn.btn-default(href="/new/hill/") New King of the Hill Game

		section.col-md-6
			h3 Robo Rugby
			p Describe robo rugby.
			a.btn.btn-default(href="/new/rugby/") New Rugby Game

		section.col-md-6
			h3 Races
			p Describe races.
			a.btn.btn-default(href="/new/race/") New Race Game
	
	hr
	
	.row
		section.col-md-6
			h3 Continue an Existing Game
			p Pick up a game right where you left off.
			a.btn.btn-default(href="/continue/") Continue Game
		
		section.col-md-6
			h3 Watch Current or Archived Games
			p Hop in and watch a game currently in progress or select an archived game to replay.
			.btn-group.hidden-xs
				a.btn.btn-default(href="/watch/current/") Watch Current Game
				a.btn.btn-default(href="/watch/archive/") Watch Archived Game
			.visible-xs
				p
					a.btn.btn-default(href="/watch/current/") Watch Current Game
				p
					a.btn.btn-default(href="/watch/archive/") Watch Archived Game